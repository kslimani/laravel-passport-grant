{
    "name": "kslimani/laravel-passport-grant",
    "description": "Laravel Passport custom grant types provider",
    "keywords": ["grant", "laravel", "passport"],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "laravel/passport": ">=4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "orchestra/testbench": "^6.25",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Sk\\Passport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sk\\Passport\\GrantTypesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
